{
  "$schema": "https://typedoc.org/schema.json",
  "sourceLinkTemplate": "https://github.com/ckb-devrel/ccc/blob/release/{path}#L{line}",
  "excludeReferences": true,
  "excludeExternals": true,
  "externalPattern": [
    "**/dist/**",
    "./node_modules/**"
  ],
  "sort": [
    "source-order",
    "alphabetical",
    "kind"
  ]
}
